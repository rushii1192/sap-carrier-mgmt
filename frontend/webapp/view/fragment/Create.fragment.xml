<core:FragmentDefinition id="_IDGenFragmentDefinition"
   xmlns="sap.m"
   xmlns:core="sap.ui.core">
   <Dialog
      id="registerDialog"
      title="Registeration for Admin" >
    <Panel id="_Panel" width="auto" class="sapUiResponsiveMargin">
        <content>
        <VBox id="verticalbox">
            <Label text="Carrier ID" />
            <Input id="ICarrierID" placeholder="Enter carrier ID" width="95%" ></Input>
 
            <Label text="Carrier Name" />
            <Input id="ICarrierName" width="95%" placeholder="Enter carrier name"></Input>
 
            <Label text="Country Code" />
            <Input id="ICountryCode"  width="95%" placeholder="Enter country code"></Input>
 
        </VBox>
        </content>
        </Panel>
      <buttons>
        <Button id="submit" text="Create" press=".onCreateSubmit" />
        <Button id="close" text="Close" press=".onCloseDialogR" />
      </buttons>
      </Dialog>
</core:FragmentDefinition>